<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="KrkrDataLoader.gui.controller.KrkrConfigController">
   <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
         <fx:include source="KrkrBaseWindow.fxml" />
         <HBox StackPane.alignment="CENTER">
            <children>
               <VBox alignment="CENTER">
                  <children>
                     <fx:include source="KrkrJsonTree.fxml" VBox.vgrow="ALWAYS" />
                     <fx:include source="KrkrCommandViewer.fxml" VBox.vgrow="ALWAYS" />
                  </children>
                  <HBox.margin>
                     <Insets bottom="10.0" left="10.0" top="10.0" />
                  </HBox.margin>
               </VBox>
               <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                  <children>
                     <fx:include source="KrkrJsonViewer.fxml" VBox.vgrow="NEVER" />
                  </children>
                  <HBox.margin>
                     <Insets left="10.0" right="10.0" />
                  </HBox.margin>
               </VBox>
            </children>
            <StackPane.margin>
               <Insets top="20.0" />
            </StackPane.margin>
         </HBox>
      </children>
   </StackPane>

</AnchorPane>
